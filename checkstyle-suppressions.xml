<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>

  <!-- TODO: Complete Javadocs -->
  <suppress checks="(MissingJavadocMethod|MissingJavadocType|SummaryJavadoc)" />

  <!-- TODO: Remove in 6.0.0 release -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]io[/\\]AOPool\.java$"
    message="'printStatisticsHTML'"
  />
  <suppress
    checks="MethodName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]md5[/\\]MD5\.java$"
    message="'(Init|Update|Final)'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]util[/\\]ShellInterpreter\.java$"
    message="'getPID'"
  />

  <!-- Using "Ex" for generic exceptions, since "E" is often "Element" -->
  <suppress
    checks="(ClassTypeParameterName|InterfaceTypeParameterName)"
    message="'Ex'"
  />

  <!-- Consistency with standard "BufferedImage.getRGB" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]awt[/\\]image[/\\]Images\.java$"
    message="'(getRGBArray|getRGB)'"
  />

  <!-- Consistency with standard "readUTF" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]io[/\\]stream[/\\]StreamableInput\.java$"
    message="'read.*UTF'"
  />

  <!-- Consistency with standard "writeUTF" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]io[/\\]stream[/\\]StreamableOutput\.java$"
    message="'write.*UTF'"
  />

  <!-- Consistency with standard "JButton" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]swing[/\\]EnterJButton\.java$"
    message="'EnterJButton'"
  />

  <!-- Consistency with standard "JDialog" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]swing[/\\]DefaultJDialog\.java$"
    message="'DefaultJDialog'"
  />

  <!-- Consistency with standard "RMIClientSocketFactory" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]rmi[/\\]RMIClientSocketFactorySSL\.java$"
    message="'RMIClientSocketFactorySSL'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]rmi[/\\]RMIClientSocketFactoryTCP\.java$"
    message="'RMIClientSocketFactoryTCP'"
  />

  <!-- Consistency with standard "RMIServerSocketFactory" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]rmi[/\\]RMIServerSocketFactorySSL\.java$"
    message="'RMIServerSocketFactorySSL'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]rmi[/\\]RMIServerSocketFactoryTCP\.java$"
    message="'RMIServerSocketFactoryTCP'"
  />

  <!-- Will keep capitalization of "AO" when used as prefix -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]io[/\\]AOPool\.java$"
    message="'AOPool'"
  />

  <!-- Will keep capitalization of "RGB" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]awt[/\\]RGBColor\.java$"
    message="'RGBColor'"
  />

  <!-- Will keep capitalization of "MD5" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]md5[/\\]MD5\.java$"
    message="'(MD5|MD5State|getMD5String|getMD5Hi|getMD5Lo)'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]md5[/\\]MD5InputStream\.java$"
    message="'MD5InputStream'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]md5[/\\]MD5OutputStream\.java$"
    message="'MD5OutputStream'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]md5[/\\]MD5Utils\.java$"
    message="'MD5Utils'"
  />

  <!-- Will keep capitalization of "QSort" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]sort[/\\]FastQSort\.java$"
    message="'FastQSort'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]sort[/\\]EQSort\.java$"
    message="'EQSort'"
  />

  <!-- Will not reorder overloads -->
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]io[/\\]AOPool\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]io[/\\]BitRate\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]io[/\\]ByteCount\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]io[/\\]TerminalWriter\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]md5[/\\]MD5\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]sort[/\\]IntegerRadixSort\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]hodgepodge[/\\]sort[/\\]QubbleSort\.java$"
  />

</suppressions>
